<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid model="com.axelor.apps.redmine.db.RedmineSyncConfig" title="Redmine sync config"
		name="redmine-sync-config-grid">

		<field name="redmineMappingFieldSelect" />

	</grid>

	<form model="com.axelor.apps.redmine.db.RedmineSyncConfig" title="Redmine sync config"
		name="redmine-sync-config-form" width="large">

		<panel name="mainPanel">
			<field name="redmineMappingFieldSelect" />
		</panel>
		<panel-related field="redmineSyncMappingList" name="redmineSyncMappingListPanel"
			form-view="redmine-sync-mapping-form" grid-view="redmine-sync-mapping-grid"
			colSpan="12" />

	</form>

</object-views>
